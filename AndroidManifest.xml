<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eps_hioa_2013.JointAttentionResearchApp"
    android:versionCode="1"
    android:versionName="0.0.1" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.eps_hioa_2013.JointAttentionResearchApp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.eps_hioa_2013.JointAttentionResearchApp.ModuleActivity"
            android:label="@string/title_activity_module"
            android:parentActivityName="com.eps_hioa_2013.JointAttentionResearchApp" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eps_hioa_2013.JointAttentionResearchApp" />
        </activity>
        <activity
            android:name="com.eps_hioa_2013.JointAttentionResearchApp.ModuleSettingsActivity"
            android:label="@string/title_activity_module_settings"
            android:parentActivityName="com.eps_hioa_2013.JointAttentionResearchApp.ModuleActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.eps_hioa_2013.JointAttentionResearchApp.ModuleActivity" />
        </activity>
    </application>

</manifest>
